  "require": {
    "php": "^7.2",
    "divineomega/password_exposed": "^2",
    "ezyang/htmlpurifier": "^4",
    "kelunik/two-factor": "^1",
    "league/commonmark": "^0.17|^1",
    "nikic/fast-route": "^1.3",
    "paragonie/anti-csrf": "^2",
    "paragonie/certainty": "^2",
    "paragonie/ciphersweet": "^1.2",
    "paragonie/constant_time_encoding": "^2",
    "paragonie/csp-builder": "^2",
    "paragonie/easydb": "^2",
    "paragonie/gpg-mailer": "^0|^1",
    "paragonie/hidden-string": "^1",
    "paragonie/ionizer": "^0|^1",
    "paragonie/random_compat": "^9",
    "paragonie/sodium_compat": "^1.6",
    "paragonie/stern": "dev-master",
    "soatok/dhole-cryptography": "^0.2.1|^1",
    "psr/http-message": "^1",
    "twig/twig": "^2",
    "zendframework/zend-servicemanager": "~2.5"
  },
  "autoload": {
    "psr-4": {
      "Soatok\\Website\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soatok\\Tests\\": "test/"
    }
  },
  "config": {
    "process-timeout" : 0
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "vimeo/psalm": "^2"
  },
  "scripts": {
    "start": "php -S localhost:8069 public/index.php",
    "test": "phpunit",
    "type-check": "psalm"
  }
